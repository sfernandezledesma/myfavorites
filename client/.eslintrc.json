{
    "extends": "react-app",
    "plugins": [
        "react-hooks"
    ],
    "rules": {
        "indent": [
            "off",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "off",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console": "off",
        "eqeqeq": "error",
        "no-unused-vars": "off",
        "react-hooks/rules-of-hooks": "error"
    }
}